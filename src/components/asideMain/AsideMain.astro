---
import { supabase } from '@/lib/supabase'; 
import AsideMainReact from './AsideMainReact';

async function fetchData() {
  const { data: variedades, error: errorVariedades } = await supabase
    .from('variedades')
    .select('id, variedad');

  if (errorVariedades ) {
    throw new Error('Error fetching Variedades ');
  }
  
  return { variedades };
}

const { variedades } = await fetchData();
---
<AsideMainReact variedades={variedades} client:load />